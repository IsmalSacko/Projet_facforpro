
<div class="">
	{{ form_start(form) }}				
	<div class="row text-center">
		<div class="col-md-4">{{ form_row(form.content) }}</div>
		<div class="col-md-4">{{ form_row(form.title) }}</div>
		<div class="col-md-4">{{ form_row(form.price) }}</div>
	</div>			
	<div class="row text-center ">
		<div class="col-md-2">{{ form_row(form.room) }}</div>
		<div class="col-md-2" id="image">{{ form_row(form.imageFile) }}</div>		
		<div class="col-md-2" id="image">{{ form_row(form.utilisateur) }}</div>

	</div>
	{# <div class="btn btn-primary" id="ajouter">ajouter</div>	 #}
	{{form_rest(form)}}	
	<div>
		<button class="btn btn-success">{{ button|default('Enregister') }}</button>
	</div>	
{{ form_end(form) }}
</div>



